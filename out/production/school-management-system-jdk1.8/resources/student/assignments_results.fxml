<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../css_files/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student.AssignmentsResults">
   <children>
      <TableView fx:id="assignmentsResultsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="300.0" minWidth="400.0" prefHeight="600.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="serialCol" maxWidth="1000.0" prefWidth="75.0" text="S.No" />
            <TableColumn fx:id="commentsCol" prefWidth="150.0" text="Comments" />
            <TableColumn fx:id="topicCol" prefWidth="150.0" text="Assignment Topic" />
            <TableColumn fx:id="obtainedMarksCol" prefWidth="150.0" text="Marks Obtained" />
            <TableColumn fx:id="totalMarksCol" prefWidth="150.0" text="Total Marks" />
            <TableColumn fx:id="percentageCol" prefWidth="150.0" text="Percentage" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
